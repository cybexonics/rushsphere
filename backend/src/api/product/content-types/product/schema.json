{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "old_price": {
      "type": "integer"
    },
    "other": {
      "type": "json"
    },
    "vendors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vendor.vendor",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "products"
    },
    "isNew": {
      "type": "boolean",
      "default": true
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "S",
        "M",
        "L",
        "XL"
      ]
    },
    "features": {
      "type": "blocks",
      "required": true
    }
  }
}
