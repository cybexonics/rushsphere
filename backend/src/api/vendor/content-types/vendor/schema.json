{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "Vendor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "profile_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "password": {
      "type": "string",
      "required": true,
      "minLength": 8,
      "config": {
        "private": false
      }
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "vendors"
    }
  }
}
